{
	menuDef
	{
		name "iwnet_account"
		rect 0 0 640 480 0 0
		style 1
		focusColor 1 1 1 1
		foreColor 1 1 1 1
		soundLoop "music_mainmenu_mp"
		fadeCycle 1
		fadeClamp 1
		fadeAmount 0.1
		popup
		onOpen
		{
			setDvar "ui_separator_show" 1;
			setDvar "ui_version_show" 1;
			open "iwnet_login_popmenu";
		}
		onClose
		{
			setDvar "ui_separator_show" 0;
			setDvar "ui_version_show" 0;
		}
	}
}
