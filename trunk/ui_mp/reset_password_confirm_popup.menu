			borderSize 2
			foreColor 0.2 0.21 0.25 1
			borderColor 0.4 0.405 0.42 1
			background "white"
			decoration 
			textScale 0.55
			visible 1
		}
		itemDef
		{
			rect -162 -42 324 84 2 2
			style 3
			foreColor 1 1 1 0.7
			background "gradient_bottom"
			decoration 
			textScale 0.55
			visible 1
		}
		itemDef
		{
			rect -158 -38 320 0 2 2
			foreColor 0.69 0.69 0.69 1
			decoration 
			autoWrapped 
			type 1
			textAlign 4
			textScale 0.375
			textStyle 6
			textFont 1
			visible 1
			exp text ( "@MENU_IWNET_RESET_PASSWORD_CONFIRM"  )
		}
		itemDef
		{
			rect -160 2 315.5 18 2 2
			style 3
			foreColor 0.9 0.9 1 0.07
